docker build --platform=linux/amd64 --pull -t orals-dev -f Dockerfile-base --build-arg rails_env=development .
docker build --platform=linux/amd64 -t orals-dev-app -f Dockerfile-dev-app --build-arg rails_env=development .
docker build --platform=linux/amd64 -t orals-dev-jobs -f Dockerfile-dev-jobs --build-arg rails_env=development .
echo
echo "Built images orals-dev, orals-dev-app, and orals-dev-jobs"