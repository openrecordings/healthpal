echo "Creating and running orals-dev-app container. Ctrl-C is safe."
rm -f tmp/pids/server.pid
docker run -d \
  --network orals-net \
  -p 3000:3000 \
  -e RAILS_ENV=development \
  --mount type=bind,source="$(pwd)",target=/app \
  --name orals-dev-app \
  orals-dev-app:latest
docker logs -f orals-dev-app
